#include <iostream>
using namespace std;

int main()
{
    int nilai [5];
    int hasil=0;
    for (int i = 0; i < 5; i++){
        cout << "Input Nilai ke-" << (i+1) << ":";
        cin >> nilai [i];
        }
    for (int i=0;i<5;i++) {
            hasil+= nilai[i];
        cout << "Cetak Nilai ke-"<<i+1<<" = "<<nilai[i]<< endl;
    }
    cout << "Total Seluruh Nilai = " << hasil;
    return 0;
}
